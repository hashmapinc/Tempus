{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "angular"
  ],
  "parser": "babel-eslint",
  "settings": {
    "import/ignore": [
      "node_modules",
      "\\.tpl\\.html$"
    ]
  },
  "globals": {
    "FileReader": true,
    "FormData": true
  },
  "env": {
    "browser": true,
    "node": true
  }
}
